import java.util.Scanner;
class Main {
   public static void main(String[] args) {
       System.out.println("Enter the Vowels:");
      char vowels = Scanner.Next().toUpperCase().charAt(0);
        switch (vowels) {
            case 'a':
                System.out.println("vowels is correct");
                break;
            case 'e':
                System.out.println("vowels is correct");
                break;
            case 'i':
                System.out.println("vowels is correct");
                break;
            case 'o':
                System.out.println("vowels is correct");
                break;
            case 'u':
                System.out.println("vowels is correct");
                break;
            default:
                System.out.println("Invalid vowels");
                break;
        }

            Scanner.Close();
    }

   }
    

