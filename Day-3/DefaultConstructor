public class DefaultConstructor{
     DefaultConstructor(){
         System.out.println("DefaultConstructor");
         
}
    public static void main(String[] args){
        //to create object
        //classname reference variable= new classname();
        DefaultConstructor ref=new DefaultConstructor();
    }
}
