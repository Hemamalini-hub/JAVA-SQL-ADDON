create table sdep( 
  sname varchar(20), 
  mark int(10), 
  rollno int(10) 
   ); 

insert into sdep(sname,mark,rollno) 
values('sneha',60,232224), 
('hari',78,232212), 
('hema',90,232224),
('sumi',80,232229),
('pavi',60,232225);

select*
from student1 left outer join Sdep
On student1.sname=Sdep.sname;
