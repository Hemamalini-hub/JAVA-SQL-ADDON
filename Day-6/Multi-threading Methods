//Multi-threading
//1.Exends thread
public class Main extends Thread{
    public void run(){
         System.out.println("Haii thread");
    }
    }
    class Execute{
      public static void main(String[] args) {
          Main m=new Main();
          m.start();//thread are execution
        //System.out.println("");
    }
    }
//2.Runnable interface
public class Main implements Runnable{
    public void run(){
         System.out.println("Haii thread");
    }
    }
    class Execute{
      public static void main(String[] args) {
          Main m=new Main();
          m.run();
        //System.out.println("");
    }
    }
